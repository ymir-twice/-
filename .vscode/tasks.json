{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "predict",
            "type": "shell",
            "command": "${command:python.interpreterPath}",
            "args": [
                "predict.py",
                "--model",
                "models",
                "--save_dir",
                "./eval/test_pred",
                "--input_shape",
                "200",
                "--if_aug",
                "1",
                "--data_dir",
                "data/images/testB3",
                "--data_stats",
                "stats/testB-meta.csv"
